#!/bin/sh
export TERMINAL=xfce4-terminal
export GTK_IM_MODULE=ibus
export XMODIFIERS=@im=ibus
export QT_IM_MODULE=ibus
ibus-daemon -drx
sh ~/.fehbg &
compton -b
sudo nm-applet&
xbindkeys
setxkbmap -option ctrl:nocaps
~/.sh/script-touchpad&
~/.sh/script-rebind_g633 > /dev/null
[ ! -s ~/.config/mpd/pid ] && mpd
